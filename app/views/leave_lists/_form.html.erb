<div class="form-group m-t-40 row">
	<%= f.label :Employee, class: "col-2 col-form-label" %>
	<div class="col-10">
		<%= f.select :employee_id, Employee.pluck(:username, :id), {include_blank: "select"}, {class: "select2 form-control custom-select",} %>
	</div>
</div>

<div class="form-group m-t-40 row">
	<%= f.label :start_date, class: "col-2 col-form-label" %>
	<div class="col-10">
		<%= f.date_field :start_date, class: "form-control",placeholder: "Start Date"%>
	</div>
</div>

<div class="form-group m-t-40 row">
	<%= f.label :end_date, class: "col-2 col-form-label" %>
	<div class="col-10">
		<%= f.date_field :end_date, class: "form-control",placeholder: "End Date" %>
	</div>
</div>

<div class="form-group m-t-40 row">
	<%= f.label :project, class: "col-2 col-form-label" %>
	<div class="col-10">
		<%= f.select :project_id, Project.pluck(:project_name,:id), {include_blank: "select"}, {class: "select2 form-control custom-select",} %>
	</div>
</div>